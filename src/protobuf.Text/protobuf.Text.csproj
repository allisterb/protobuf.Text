<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
    <RootNamespace>Protobuf.Text</RootNamespace>
    <AssemblyName>Protobuf.Text</AssemblyName>
    <Version>0.4.0</Version>
    <Company>SciSharp STACK</Company>
    <Product>protobug.Text</Product>
    <Authors>Kerry Jiang and other contributors</Authors>
    <Description>Text format support for google's protobuf C# library</Description>
    <Copyright>Apache 2.0</Copyright>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/SciSharp/protobuf.Text</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SciSharp/protobuf.Text</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>C#,protobuf,text,text format</PackageTags>
    <PackageReleaseNotes>Upgrade Google.Protobuf to 3.10.1.</PackageReleaseNotes>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/44989469?s=200&amp;v=4</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="$(ProtobufVersion)" />    
  </ItemGroup>
</Project>